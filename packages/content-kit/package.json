{
  "name": "@docufy/content-kit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset/index.d.ts",
      "import": "./dist/preset/index.js"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "import": "./dist/renderer/index.js"
    },
    "./markdown": {
      "types": "./dist/markdown/index.d.ts",
      "import": "./dist/markdown/index.js"
    },
    "./extensions/code-block": {
      "types": "./dist/extensions/code-block/index.d.ts",
      "import": "./dist/extensions/code-block/index.js"
    },
    "./extensions/image": {
      "types": "./dist/extensions/image/index.d.ts",
      "import": "./dist/extensions/image/index.js"
    },
    "./extensions/custom-keymap": {
      "types": "./dist/extensions/custom-keymap/index.d.ts",
      "import": "./dist/extensions/custom-keymap/index.js"
    },
    "./plugins/upload-images": {
      "types": "./dist/plugins/upload-images.d.ts",
      "import": "./dist/plugins/upload-images.js"
    },
    "./lowlight": {
      "types": "./dist/lowlight/index.d.ts",
      "import": "./dist/lowlight/index.js"
    },
    "./styles.css": {
      "default": "./dist/styles/content-kit.css"
    },
    "./extensions/slash-command": {
      "types": "./dist/extensions/slash-command/index.d.ts",
      "import": "./dist/extensions/slash-command/index.js"
    }
  },
  "peerDependencies": {
    "@tiptap/core": "^3.6.1",
    "@tiptap/react": "^3.6.1",
    "@tiptap/starter-kit": "^3.6.1",
    "@tiptap/static-renderer": "^3.6.1",
    "react": "^18 || ^19"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@tiptap/extension-details": "^3.6.1",
    "@tiptap/extension-emoji": "^3.6.1",
    "@tiptap/extension-table": "^3.6.1",
    "@tiptap/extension-task-item": "^3.6.1",
    "@tiptap/extension-task-list": "^3.6.1",
    "@tiptap/suggestion": "^3.6.1",
    "cheerio": "^1.1.2",
    "cmdk": "^1.1.1",
    "cssnano": "^7.1.1",
    "dompurify": "^3.2.7",
    "highlight.js": "^11.11.1",
    "html-entities": "^2.6.0",
    "jotai": "^2.14.0",
    "lowlight": "^3.3.0",
    "node-html-parser": "^7.0.1",
    "postcss": "^8.5.6",
    "postcss-prefix-selector": "^2.1.1",
    "prosemirror-state": "^1.4.3",
    "sanitize-html": "^2.17.0",
    "shiki": "^3.13.0",
    "tippy.js": "^6.3.7",
    "tiptap-extension-auto-joiner": "^0.1.3",
    "tiptap-extension-global-drag-handle": "^0.1.18",
    "tunnel-rat": "^0.1.2"
  },
  "devDependencies": {
    "@docufy/tsconfig": "workspace:*",
    "@tiptap/core": "^3.6.1",
    "@tiptap/extension-code-block-lowlight": "^3.6.1",
    "@tiptap/extension-image": "^3.6.1",
    "@tiptap/pm": "^3.6.5",
    "@tiptap/react": "^3.6.1",
    "@tiptap/starter-kit": "^3.6.1",
    "@tiptap/static-renderer": "^3.6.1",
    "@types/postcss-prefix-selector": "^1.16.3",
    "@types/sanitize-html": "^2.16.0",
    "typescript": "^5.6.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && mkdir -p dist/styles && cp -R src/styles/. dist/styles/",
    "dev": "tsc --watch -p tsconfig.build.json",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write ."
  }
}
