NODE_ENV=development
# Database connection URL for PostgreSQL
# This should match the database configuration in docker-compose.yaml
DATABASE_URL=postgresql://postgres:password@localhost:54321/electric

# Authentication secret - REQUIRED in production
# Generate a strong secret (minimum 32 characters)
BETTER_AUTH_SECRET=

# Production Electric Cloud Configuration (optional)
# Get these from https://dashboard.electric-sql.cloud/
ELECTRIC_SOURCE_ID=
ELECTRIC_SOURCE_SECRET=

# Resend
RESEND_API_KEY=
PUBLIC_URL=https://web.localhost

BLOB_READ_WRITE_TOKEN=
VITE_PUBLIC_VERCEL_BLOB_STORE_ID=
VITE_PUBLIC_VERCEL_BLOB_BASE_URL=

VERCEL_RENDERER_PROJECT=
VERCEL_TOKEN=
INNGEST_SIGNING_KEY=
INNGEST_EVENT_KEY=

# New for docs as code, later to add in docker compose and sst
GITHUB_APP_ID=
GITHUB_APP_CLIENT_ID=
GITHUB_APP_CLIENT_SECRET=
GITHUB_APP_PRIVATE_KEY= # one-line PEM with literal \n (e.g. "-----BEGIN...\\n...\\n-----END RSA PRIVATE KEY-----\\n")
VITE_PUBLIC_GITHUB_APP_SLUG=
